page {
    align-items: center;
}

.container {
    width: 454rpx;
    height: 454rpx;
    border: 20rpx solid #0f4ab8;
    overflow: hidden;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    align-content: space-between;
    border-radius: 20rpx;
}

.v1 {
    width: 150rpx;
    height: 150rpx;
    font-size: 70rpx;
    font-weight: 600;
    position: relative;
    border-radius: 15rpx;
}

.v1 view {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 10;
}

.v2 {
    width: 150rpx;
    height: 150rpx;
    background: #fff;
}

.v3 {
    font-size: 34rpx;
    margin: 20rpx 0;
}

.v4 {
    font-size: 34rpx;
    margin-bottom: 20rpx;
}

.v5 {
    font-size: 34rpx;
    margin-bottom: 30rpx;
}
.game {
    width: 300rpx;
    height: 80rpx;
    background: #126ace;
    color: #fff;
    border-radius: 20rpx;
    font-size: 36rpx;
}

.reset {
    width: 300rpx;
    height: 80rpx;
    background: #126ace;
    color: #fff;
    border-radius: 20rpx;
    font-size: 36rpx;
    margin: 20rpx 0;
}